{
    "name": "pallari/geoname-bundle",
    "description": "A Symfony bundle for GeoNames with incremental daily updates support.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Gian-Paolo Pallari",
            "email": "gianpaolo.pallari@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "symfony/framework-bundle": "^6.4|^7.0|^8.0",
        "doctrine/doctrine-bundle": "^2.10",
        "doctrine/orm": "^3.0",
        "symfony/http-client": "^6.4|^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^13.0",
        "symfony/phpunit-bridge": "^8.0",
        "symfony/yaml": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Pallari\\GeonameBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pallari\\GeonameBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
